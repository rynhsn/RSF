	</ItemGroup>

	
	<ItemGroup>
		<Reference Include="R_APIBackEnd">
		  <HintPath>..\..\..\Library\R_APIBackEnd.dll</HintPath>
		</Reference>
		<Reference Include="R_ApplicationBack">
			<HintPath>..\..\..\Library\R_ApplicationBack.dll</HintPath>
		</Reference>
		<Reference Include="R_CommonFrontBackAPI">
			<HintPath>..\..\..\Library\R_CommonFrontBackAPI.dll</HintPath>
		</Reference>
		<Reference Include="R_MultiTenantDb">
			<HintPath>..\..\..\Library\R_MultiTenantDb.dll</HintPath>
		</Reference>
		<Reference Include="R_ReportServerClient">
			<HintPath>..\..\..\Library\R_ReportServerClient.dll</HintPath>
		</Reference>
		<Reference Include="R_ReportServerCommon">
			<HintPath>..\..\..\Library\R_ReportServerCommon.dll</HintPath>
		</Reference>
		<Reference Include="R_APICommon">
			<HintPath>..\..\..\Library\R_APICommon.dll</HintPath>
