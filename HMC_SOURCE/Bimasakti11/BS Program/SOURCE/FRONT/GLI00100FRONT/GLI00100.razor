                    ValueField="@nameof(GLI00100BudgetDTO.CBUDGET_NO)"
                    TextField="@nameof(GLI00100BudgetDTO.CBUDGET_NAME_DISPLAY)"
                    Enabled="true"/>
    </R_ItemLayout>
    <R_Button Enabled="@(_viewModel.GLAccountList.Count > 0 && _viewModel.CenterCode != null && _viewModel.BudgetNo != null)"
