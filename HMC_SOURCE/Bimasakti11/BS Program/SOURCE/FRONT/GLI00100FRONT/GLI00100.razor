        Refresh
    </R_Button>
    <R_Popup Enabled="@(_viewModel.GLAccountList.Count > 0 && _viewModel.CenterCode != null && _viewModel.BudgetNo != null)"
             R_Before_Open_Popup="BeforeOpenPopupPrint">
