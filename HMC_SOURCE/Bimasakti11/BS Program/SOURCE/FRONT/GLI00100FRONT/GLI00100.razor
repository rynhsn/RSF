            DataSource="_viewModel.GLAccountList"
            Pageable="true"
            R_ConductorSource="_conductorRef"
            R_GridType="R_eGridType.Navigator"
            R_ServiceGetListRecord="GetListRecord"
            Width="350px"
            Height="560px">
        <R_GridColumns>
            <R_GridTextColumn FieldName="@nameof(GLI00100AccountGridDTO.CGLACCOUNT_NO)" HeaderText="Account No." Width="120px" TextAlignment="R_eTextAlignment.Left"/>
            <R_GridTextColumn FieldName="@nameof(GLI00100AccountGridDTO.CGLACCOUNT_NAME)" HeaderText="Account Name" Width="230px" TextAlignment="R_eTextAlignment.Left"/>
        </R_GridColumns>
    </R_Grid>

    <R_StackLayout Row Spacing="2">
        <R_ItemLayout col="4">
            <R_Label>Account Type</R_Label>
        </R_ItemLayout>
        <R_ItemLayout>
            <R_RadioGroup Data="@_viewModel.CBSIS"
                          @bind-Value="_viewModel.Data.CBSIS"
                          ValueField="Key"
                          TextField="Value"
                          Enabled="false"/>
        </R_ItemLayout>
    </R_StackLayout>

    <R_StackLayout Row Spacing="2">
        <R_ItemLayout col="4">
            <R_Label>Normal Journal</R_Label>
        </R_ItemLayout>
        <R_ItemLayout>
            <R_RadioGroup Data="@_viewModel.CDBCR"
                          @bind-Value="_viewModel.Data.CDBCR"
                          ValueField="Key"
                          TextField="Value"
                          Enabled="false"/>
        </R_ItemLayout>
    </R_StackLayout>
</R_ItemLayout>

<R_ItemLayout>
<R_StackLayout Row Spacing="2" Class="mb-1">
    <R_ItemLayout col="2">
        <R_Label>Account</R_Label>
    </R_ItemLayout>
    <R_ItemLayout col="3">
        <R_TextBox @bind-Value="_viewModel.Data.CGLACCOUNT_NO" Enabled="false"/>
    </R_ItemLayout>
    <R_ItemLayout col="4">
        <R_TextBox @bind-Value="_viewModel.Data.CGLACCOUNT_NAME" Enabled="false"/>
    </R_ItemLayout>
</R_StackLayout>

<R_StackLayout Row Spacing="2" Class="mb-1">
    <R_ItemLayout col="2">
        <R_Label>Year</R_Label>
    </R_ItemLayout>
    <R_ItemLayout col="3">
        <R_NumericTextBox @bind-Value="_viewModel.Year"
                          ThousandSeparator="false"
                          Min="@_viewModel.GSMPeriod.IMIN_YEAR"
                          Max="@_viewModel.GSMPeriod.IMAX_YEAR"
                          OnChanged="RefreshBudgetCombo"/>
    </R_ItemLayout>

    <R_ItemLayout>
        <R_Label>Currency Type</R_Label>
    </R_ItemLayout>
    <R_ItemLayout>
        <R_RadioGroup Data="@_viewModel.CCURRENCY_TYPE"
                      @bind-Value="_viewModel.CurrencyTypeValue"
                      ValueField="Key"
                      TextField="Value"
                      Enabled="true"
                      OnChanged="RefreshBudgetCombo"/>
    </R_ItemLayout>
</R_StackLayout>

<R_StackLayout Row Spacing="2" Class="mb-1">
    <R_ItemLayout col="2">
        <R_Label>Center</R_Label>
    </R_ItemLayout>
    <R_ItemLayout col="3">
        <R_TextBox @bind-Value="_viewModel.CenterCode" Enabled="false"/>
    </R_ItemLayout>
    <R_Lookup R_Before_Open_Lookup="BeforeOpenLookupCenter"
              R_After_Open_Lookup="AfterOpenLookupCenter">
    </R_Lookup>
    <R_ItemLayout col="3">
        <R_TextBox @bind-Value="_viewModel.CenterName" Enabled="false"/>
    </R_ItemLayout>
</R_StackLayout>

<R_StackLayout Row Spacing="2" Class="mb-1">
    <R_ItemLayout col="2">
        <R_Label>Budget No.</R_Label>
    </R_ItemLayout>
    <R_ItemLayout col="7">
