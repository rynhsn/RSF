            R_ServiceGetListRecord="GetListRecord"
            Width="350px"
            Height="520px">
        <R_GridColumns>
            <R_GridTextColumn FieldName="@nameof(GLI00100AccountGridDTO.CGLACCOUNT_NO)" HeaderText="@_localizer["LABEL_ACCNO"]" Width="120px" TextAlignment="R_eTextAlignment.Left"/>
            <R_GridTextColumn FieldName="@nameof(GLI00100AccountGridDTO.CGLACCOUNT_NAME)" HeaderText="@_localizer["LABEL_ACCNAME"]" Width="270px" TextAlignment="R_eTextAlignment.Left"/>
        </R_GridColumns>
