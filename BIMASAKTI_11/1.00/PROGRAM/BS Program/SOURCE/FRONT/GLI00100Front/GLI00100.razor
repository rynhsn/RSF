            <R_Label>@_localizer["LABEL_ACCTYPE"]</R_Label>
        </R_ItemLayout>
        <R_ItemLayout>
            <R_RadioGroup Data="@_viewModel.CBSIS"
                          @bind-Value="_viewModel.Data.CBSIS"
                          ValueField="Key"
                          TextField="Value"
                          Layout="R_eRadioGroupLayout.Vertical"
                          Enabled="false"/>
        </R_ItemLayout>
    </R_StackLayout>

    <R_StackLayout Row Spacing="2">
        <R_ItemLayout col="4">
            <R_Label>@_localizer["LABEL_NRMLJOURNAL"]</R_Label>
        </R_ItemLayout>
        <R_ItemLayout>
