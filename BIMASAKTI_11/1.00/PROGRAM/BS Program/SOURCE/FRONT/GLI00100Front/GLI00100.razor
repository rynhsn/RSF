                          @bind-Value="_viewModel.Data.CDBCR"
                          ValueField="Key"
                          TextField="Value"
                          Layout="R_eRadioGroupLayout.Horizontal"
                          Enabled="false"/>
        </R_ItemLayout>
    </R_StackLayout>
</R_ItemLayout>

